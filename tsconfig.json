{
  "compilerOptions": {
    "target": "es2018",
    "module": "esnext",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "lib": [
      "es2017",
      "dom",
      "dom.iterable"
    ],
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules",
    "dist",
  ],
}
